<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <com.example.hencoder.pr01.CameraView
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <ImageView
        android:id="@+id/imageview"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:src="@drawable/view_anim_frame"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <ImageView
        android:id="@+id/view2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/circle_shape"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <com.example.hencoder.view.MaterialEditTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="hello"
        android:visibility="gone"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <com.example.hencoder.view.TabLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="hello world"/>

        <com.example.hencoder.view.OneMeasureView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="hello world"/>

        <com.example.hencoder.view.OneMeasureView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="hello world"/>

        <com.example.hencoder.view.OneMeasureView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="hello world"/>

        <com.example.hencoder.view.OneMeasureView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="hello world fgfsd"/>

        <com.example.hencoder.view.OneMeasureView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="hello"/>


        <com.example.hencoder.view.OneMeasureView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="hello worldddd"/>
    </com.example.hencoder.view.TabLayout>

    <com.example.hencoder.view.ScalableImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"/>

    <com.example.hencoder.view.MultiTouchView01
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</FrameLayout>